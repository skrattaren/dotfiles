colorscheme solarized
set gui=none,tabs
nmap s :tabopen<Space>google<Space>
nmap S :open<Space>google<Space>
nmap F ;t
nmap h :back<Return>
nmap l :forward<Return>
nmap H :back!<Return>
nmap C :set nohlsearch<Return>
map <C-Tab> i<C-Tab>
set "editor=gvim -f"
set hintchars=hjklasdf
set urlseparator=" \\| "
ignorekeys add -except b,g,t,T,;,:,s,<C-p>,<C-n>,<C-6>,u bazqux.com

" about:config
set! signon.autofillForms=false

